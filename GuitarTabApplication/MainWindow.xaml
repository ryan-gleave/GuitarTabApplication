<Window x:Class="GuitarTabApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GuitarTabApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ContentControl Content="{Binding SelectedViewModel}" Margin="5" />
        <Button Margin="5" IsEnabled="False" VerticalAlignment="Bottom" HorizontalAlignment="Left" Name="btnBack" Click="btnBack_Click">Back</Button>
    </Grid>
</Window>
